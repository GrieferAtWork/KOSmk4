{
	"files.exclude": {
		".vs": true,
		".vscode": true,
		"**.dec": true,
		"**.latest": true,
		"**/__ice__": true
	},
	"C_Cpp.autoAddFileAssociations": false,
	"C_Cpp.debugShortcut": false,
	"C_Cpp.formatting": "clangFormat",
	"C_Cpp.preferredPathSeparator": "Forward Slash",
	"C_Cpp.workspaceSymbols": "Just My Code",
	"files.associations": {
		"*.c" : "cpp",
		"*.ctest" : "cpp",
		"*.h" : "cpp",
		"*.c.inl" : "cpp",
		"*.sh" : "shellscript",
	}
}