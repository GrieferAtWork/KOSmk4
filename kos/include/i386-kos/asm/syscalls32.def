/* Define type compatibilities between x86_64 and i386 */
%[typecompat(i386:[sigset_t,__sigset_struct] = *:[__sigset_struct,sigset_t])]
%[typecompat(i386:[fd_set,__fd_set_struct] = *:[fd_set,__fd_set_struct])]
