E(SIGHUP)    /* Hangup (POSIX). */
E(SIGINT)    /* Interrupt (ANSI). */
E(SIGQUIT)   /* Quit (POSIX). */
E(SIGILL)    /* Illegal instruction (ANSI). */
E(SIGTRAP)   /* Trace trap (POSIX). */
E(SIGABRT)   /* Abort (ANSI). */
E(SIGBUS)    /* BUS error (4.2 BSD). */
E(SIGFPE)    /* Floating-point exception (ANSI). */
E(SIGKILL)   /* Kill, unblockable (POSIX). */
E(SIGUSR1)   /* User-defined signal 1 (POSIX). */
E(SIGSEGV)   /* Segmentation violation (ANSI). */
E(SIGUSR2)   /* User-defined signal 2 (POSIX). */
E(SIGPIPE)   /* Broken pipe (POSIX). */
E(SIGALRM)   /* Alarm clock (POSIX). */
E(SIGTERM)   /* Termination (ANSI). */
E(SIGSTKFLT) /* Stack fault. */
E(SIGCHLD)   /* Child status has changed (POSIX). */
E(SIGCONT)   /* Continue (POSIX). */
E(SIGSTOP)   /* Stop, unblockable (POSIX). */
E(SIGTSTP)   /* Keyboard stop (POSIX). */
E(SIGTTIN)   /* Background read from tty (POSIX). */
E(SIGTTOU)   /* Background write to tty (POSIX). */
E(SIGURG)    /* Urgent condition on socket (4.2 BSD). */
E(SIGXCPU)   /* CPU limit exceeded (4.2 BSD). */
E(SIGXFSZ)   /* File size limit exceeded (4.2 BSD). */
E(SIGVTALRM) /* Virtual alarm clock (4.2 BSD). */
E(SIGPROF)   /* Profiling alarm clock (4.2 BSD). */
E(SIGWINCH)  /* Window size change (4.3 BSD, Sun). */
E(SIGIO)     /* I/O now possible (4.2 BSD). */
E(SIGPWR)    /* Power failure restart (System V). */
E(SIGSYS)    /* Bad system call. */
